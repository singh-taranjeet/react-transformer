{
  "name": "@react-transformer/replacer",
  "version": "1.0.19",
  "description": "Transform simple string patterns to React components",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/replacer.esm.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "npm run test && npm run prettier && npm run lint",
    "test": "jest --config jestconfig.json",
    "test:dev": "jest --config jestconfig.json --watchAll",
    "test:coverage": "jest --config jestconfig.json --coverage",
    "pub": "npm publish --access public",
    "lint:fix": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" --fix",
    "start": "tsdx watch",
    "build": "tsdx build",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "typescript",
    "string to react components"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Taranjeet Singh",
  "license": "MIT",
  "size-limit": [
    {
      "path": "dist/trans.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/trans.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "@babel/core": "^7.21.8",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/addons": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "babel-loader": "^9.1.2",
    "husky": "^8.0.3",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/singh-taranjeet/react-transformer.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
